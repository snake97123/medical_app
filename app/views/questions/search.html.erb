<div class="wrapper">
<%= render "shared/header" %>
<body>
   <div class="question-lists">検索結果
    <div class="question-counts">
    <%= form_with(url: search_questions_path, local:true, method: :get, class: "search-form") do |form| %>
   <%= form.text_field :keyword, placeholder: "投稿を検索する", class: "search-input" %>
   <%= form.submit "検索", class: "btn" %>
   <% end %>
   </div>
   <div class="contents row">
    <% @questions.each do |question|  %>
      <li class="list">
         <%= link_to  "#{question.title}", question_path(question.id)  %>
      <h1 class="question-list">
           質問した日
         <%=l question.created_at %>
      </h1>
     <% end %>
    </div>
    </body> 
    </div>
    <%= render "shared/footer"%>